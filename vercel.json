{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.2"
        },
        "public/*.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "/api/index.php"
        },
        {
            "src": "/public/index.php",
            "dest": "/public/index.php"
        },
        {
            "src": "/public/insertar.php",
            "dest": "/public/insertar.php"
        }
    ]
}
